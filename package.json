{
  "name": "boilerplate-2021",
  "version": "1.0.0",
  "description": "Yet another boilerplate for TypeScript and React developers.",
  "main": "dist/index.js",
  "module": "dist/es/index.js",
  "types": "dist/es/index.d.js",
  "author": "Henry Li <henry1943@163.com>",
  "repository": {
    "url": "https://github.com/magiccube/boilerplate-2021"
  },
  "license": "MIT",
  "scripts": {
    "build:webpack": "webpack --config scripts/webpack/webpack.prod.config.js",
    "build:es": "tsc",
    "build:gh-pages": "npm run build:webpack",
    "build": "npm run build:webpack && npm run build:es",
    "dev": "webpack serve --config scripts/webpack/webpack.dev.config.js",
    "start": "npm run dev",
    "prepare": "husky install",
    "deploy": "npm run build:gh-pages && echo && echo 'Publishing GitHub pages...' && gh-pages -d dist"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
